#include<iostream>
using namespace std;
int main(){
    int N, M;
    cin >> N >> M;
    int x1, y1, x2, y2;
    cin >> x1 >> y1 >> x2 >> y2;
    int dx1, dy1, dx2, dy2;
    cin >> dx1 >> dy1 >> dx2 >> dy2; 
    int t;
    cin >> t;
    for (int i = 0; i < t; i++)
    {
        int px1 = x1 + dx1;
        int py1 = y1 + dy1;
        int px2 = x2 + dx2;
        int py2 = y2 + dy2;
        if (px1 < 0 || px1 > N)
        {
            dx1 = - dx1;
            px1 = x1 + dx1;
        }
        if (py1 < 0 || py1 > M)
        {
            dy1 = - dy1;
            py1 = y1 + dy1;
        }
        if (px2 < 0 || px2 > N)
        {
            dx2 = - dx2;
            px2 = x2 + dx2;
        }
        if (py2 < 0 || py2 > M)
        {
            dy2 = - dy2;
            py2 = y2 + dy2;
        }
        if (px1 == px2 && py1 == py2)
        {
            swap(dx1, dx2);
            swap(dy1, dy2);
        }
        x1 = px1;
        y1 = py1;
        x2 = px2;
        y2 = py2;

    }
    cout << x1 << " " << y1 << " " << x2 << " " << y2;
}
