#include <iostream>
using namespace std;
int count(int a){
    if (a == 1 || a == 3){
        return 1;
    }else if (a == 0){
        return 0;
}else if(a < 0){
return 0;
}
    
    return count(a - 5) + count(a - 3);
}
int main(){
    int N;
    cin >> N;
    int ans;
    ans = count(N);
    
cout << ans;
}
